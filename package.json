	"scripts": {
		"postinstall": "if [ $CLIENT_ENV ]; then npm run postinstall-client; elif [ $SERVER_ENV ]; then npm run postinstall-server; else echo no environment detected, please set CLIENT_ENV or SERVER_ENV; fi",
		"postinstall-client": "cd sc2006-web && npm ci && npm run build",
		"postinstall-server": "cd sc2006-common && npm ci && npm run build && cd ../sc2006-backend && npm install && npm run build",
		"start": "if [ $CLIENT_ENV ]; then npm run start:web; elif [ $SERVER_ENV ]; then npm run start:server; else echo no environment detected, please set CLIENT_ENV or SERVER_ENV; fi",
		"start:web": "cd sc2006-web && npm run start",
		"start:server": "cd sc2006-backend && npm run start:prod",
		"dev": "if [ $CLIENT_ENV ]; then npm run dev:web; elif [ $SERVER_ENV ]; then npm run dev:server; else echo no environment detected, please set CLIENT_ENV or SERVER_ENV; fi",
		"dev:web": "cd sc2006-web && npm run dev",
		"dev:server": "cd sc2006-backend && npm run start:dev",
		"test": "echo \"Error: no test specified\" && exit 1",
